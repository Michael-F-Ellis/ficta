{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "AIT",
	"patterns": [
		{
			"include": "#comments"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.double-slash.ait",
					"match": "((?://).*)$"
				},
				{
					"name": "comment.block.ait",
					"begin": "/\\*",
					"end": "\\*/"
				}
			]
		}
	},
	"scopeName": "source.ait"
}